<html>
  <head>
    <meta charset="utf-8">
    <title>Linux Dungeon</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/room.css">
  </head>
  <body>
    <div class="root">
      <% include ./appbar.ejs {user, score} %>
      <h1 class="text" align="center">正解</h1>
      <br>
      <!-- /dungeonにリクエスト -->
      <form action="/dungeon" method="post" autocomplete="off" align="center">
        <input type="submit" class="button" value="部屋を出る">
        <input id="user" type="hidden" name="user" value=<%= user %>>
      </form>
    </div>
  </body>
</html>
