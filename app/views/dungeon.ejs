<html>
  <head>
    <meta charset="utf-8">
    <title>Linux Dungeon</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/dungeon.css">
    <script type="text/javascript" src="/scripts/dungeon.js"></script>
  </head>
  <body onload="init()">
    <div class="root">
      <% include ./appbar.ejs {user, score} %>
      <input id="user" type="hidden" name="user" value="<%= user %>" form="room">
      <canvas id="floor" width="1000" height="500"></canvas>
      <img id="kabe" src="/images/kabe.png" style="display:none" />
      <img id="monn" src="/images/monn.png" style="display:none" />
      <img id="yuusya" src="/images/yuusya.png" style="display:none" />
      <table style="display:none">
        <tr><td></td><td><button onclick="up()">U</button></td><td></td></tr>
        <tr><td><button onclick="left()">L</button></td><td></td>
            <td><button onclick="right()">R</button></td></tr>
        <tr><td></td><td><button onclick="down()">D</button></td><td></td></tr>
      </table>
      <div id="modal-content-treasure-get"></div>
      <% include ./treasure-box.ejs %>
    </div>
  </body>
</html>
